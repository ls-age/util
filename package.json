{
  "name": "@ls-age/util",
  "version": "0.4.1",
  "description": "ls-age utilities",
  "main": "out/index.js",
  "bin": {
    "ls-util": "out/bin/ls-util.js"
  },
  "scripts": {
    "compile": "babel src --out-dir out",
    "compile:watch": "npm run compile -- --watch",
    "docs": "esdoc -c esdoc.json",
    "lint": "eslint src",
    "prepublish": "npm run compile && npm run docs",
    "test": "mocha --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ls-age/util.git"
  },
  "author": "Lukas Hechenberger <npm@ls-age.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ls-age/util/issues"
  },
  "homepage": "https://github.com/ls-age/util#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "cli-spinners": "^2.0.0",
    "conventional-github-releaser": "^3.1.2",
    "inquirer": "^6.2.2",
    "node-status": "^1.0.0",
    "standard-version": "^5.0.2",
    "tildify": "^1.2.0",
    "which": "^1.2.12",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-system-import-transformer": "^4.0.0",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.18.0",
    "esdoc": "^1.1.0",
    "eslint": "^5.15.3",
    "eslint-config-lsage": "^0.3.0",
    "mocha": "^6.0.2",
    "unexpected": "^11.2.0"
  },
  "babel": {
    "presets": [
      "node6"
    ],
    "plugins": [
      "system-import-transformer"
    ]
  },
  "eslintConfig": {
    "extends": "lsage"
  }
}
